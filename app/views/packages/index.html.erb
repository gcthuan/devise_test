<% provide(:title, 'All Packages') %>
<%= link_to "Add new package", "/packages/new", class: "btn btn-medium btn-primary" %>
<h1>All packages</h1>

<%= will_paginate %>

<ul class="packages">
  <% @packages.each do |package| %>
    <li>
      <%= link_to package.name, package %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>